{
    "name": "anexia/param-parser",
    "description": "Parses param placeholders within an arbitrary string",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "minimum-stability": "stable",
    "homepage": "https://github.com/anexia/php-param-parser",
    "authors": [
        {
            "name": "Andreas Stocker",
            "email": "AStocker@anexia-it.com"
        }
    ],
    "autoload": {
        "files": [
            "src/parse.php"
        ],
        "psr-4": {
            "Anexia\\ParamParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anexia\\ParamParser\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=7.5",
        "squizlabs/php_codesniffer": ">=3.6"
    }
}
